{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/*",
      "dest": "src/api/:splat"
    },
    {
      "src": "/(.*)",
      "dest": "src/index.html"
    }
  ],
  "files": [
    "public/**/*",
    "src/url.js"
  ]
}